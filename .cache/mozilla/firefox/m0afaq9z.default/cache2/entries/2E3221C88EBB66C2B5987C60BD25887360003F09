(function(symbolName){Symbol.bindSymbolAction(compId,symbolName,"creationComplete",function(sym,e){var CONST={delay:(wormwood.ad.device.isChrome||wormwood.ad.device.isSafari)?1:20,forward:"f",backwards:"b"};var animSets={fade:{showA:{properties:{"opacity":1}},hideA:{properties:{"opacity":0}}},slide_vertical:{showA:{properties:{"transform":"translate(0,0)"}},hideA:{properties:{"transform":"translate(0, 100%)"}},hideB:{properties:{"transform":"translate(0, -100%)"}}},slide_horizontal:{showA:{properties:{"transform":"translate(0,0)"},},hideA:{properties:{"transform":"translate(100%,0)"}},hideB:{properties:{"transform":"translate(-100%,0)"}}},slide_horizontal_2:{showA:{properties:{"transform":"translateX(0) rotateY(0)"}},hideA:{properties:{"transform":"translateX(100%) rotateY(75deg)"},},hideB:{properties:{"transform":"translateX(-100%) rotateY(-75deg)"}},perspective:"600px"},slide_horizontal_3:{showA:{properties:{"transform":"translate(0,0) rotateY(0) translateZ(0)"}},hideA:{properties:{"transform":"translate(100%,0) rotateY(-70deg) translateZ(-200px)"}},hideB:{properties:{"transform":"translate(-100%,0) rotateY(70deg) translateZ(-200px)"}},perspective:"400px"},slide_horizontal_4:{showA:{properties:{"transform":"rotateY(0)"},initialStyles:{"transform-origin":"100% 50%","-webkit-transform-origin":"100% 50%"}},hideA:{properties:{"transform":"rotateY(120deg)"},initialStyles:{"transform-origin":"100% 50%","-webkit-transform-origin":"100% 50%",}},showB:{properties:{"transform":"rotateY(0)"},initialStyles:{"transform-origin":"0% 50%","-webkit-transform-origin":"0% 50%",}},hideB:{properties:{"transform":"rotateY(-120deg)"},initialStyles:{"transform-origin":"0% 50%","-webkit-transform-origin":"0% 50%"}},perspective:800},slide_horizontal_rotate:{showA:{properties:{"transform":"translateX(0) rotate(0)",},delay:50},hideA:{properties:{"transform":"translateX(160%) rotate(90deg)"},},hideB:{properties:{"transform":"translateX(-160%) rotate(-90deg)"}}},slide_horizontal_flip:{showA:{properties:{"transform":"translateZ(0) rotateY(0)","opacity":"1"},initialStyles:{"transform-origin":"50% 50%"},easing:"ease-out",init:function(instance){this.delay=instance.conf.animationDuration;}},hideA:{properties:{"transform":"translateZ(100px) rotateY(90deg)",opacity:"0.4"},initialStyles:{"transform-origin":"50% 50%"},easing:"ease-in",init:function(){if(wormwood.ad.device.isSafari===false&&wormwood.ad.device.isChrome===false&&wormwood.ad.device.isMobile===false){this.properties.opacity="1";}}},hideB:{properties:{"transform":"translateZ(-100px) rotateY(-90deg)",opacity:"0.4"},initialStyles:{"transform-origin":"50% 50%"},easing:"ease-in",init:function(){if(wormwood.ad.device.isSafari===false&&wormwood.ad.device.isChrome===false&&wormwood.ad.device.isMobile===false){this.properties.opacity="1";}}},perspective:"1200px"}};function makeTag(type,id,cls){var el=document.createElement(type);if(id){el.id=id;}
if(cls){el.className=cls;}
return el;}
function addLengthUnit(str){return(/px|%|em/g).test(str)?str:str+"px";}
function defer(fn,t){return setTimeout(fn,t);}
function setIndex(self,i){self.conf.previous=self.conf.current;self.conf.current=parseInt(i);}
function getDirection(self,i,d){var dir=d;if(!dir){if(i>self.conf.current){dir=CONST.forward;}
else if(i<self.conf.current){dir=CONST.backwards;}}
return dir;}
function prevIndex(self){var i=self.conf.current-1;if(i<0){i=self.conf.length-1;}
return i;}
function nextIndex(self){var i=self.conf.current+1;if(i>self.conf.length-1){i=0;}
return i;}
function beginAnimations(self){self.slides.update();if(self.pageControl){self.pageControl.update();}
fireCallback(self,{type:"onShowSlide",dir:self.conf.direction,index:self.conf.current});}
function onHover(){this.stopTimer();this.conf.isMouseover=true;}
function onBlur(){if(this.conf.isAnimating===false){this.startTimer();}
this.conf.isMouseover=false;}
function setupCallbacks(self,args){self.callbacks=$.extend({},args);}
function fireCallback(self,args){if(self.callbacks[args.type]){self.callbacks[args.type](args);}}
function buildGallery(self,callbacks){buildConf(self);self.el=self.view.getSymbolElement();if(wormwood.ad.device.isMobile===false){self.el.on("mouseover",onHover.bind(self));self.el.on("mouseout",onBlur.bind(self));}
self.slides=new SlideManager(self);if(self.conf.showArrows===true){self.arrows=new ArrowManager(self);}
else{self.model.leftArrow.remove();self.model.rightArrow.remove();}
if(self.conf.showPageControl===true){self.pageControl=new PageControl(self);}
else{self.model.dot_on.remove();self.model.dot_off.remove();self.model.pageControl.remove();}
if(self.conf.timeInterval){self.timer=new Timer(self);self.timer.start();}
if(callbacks){setupCallbacks(self,callbacks);}
self.onReady(self);beginAnimations(self);}
function buildAnimationSet(self){var set=self.conf.animationSet,key,current;for(key in set){current=set[key];if(current.init&&(typeof current.init==="function")){current.init(self);}}}
function buildConf(self){var conf=self.conf={};conf.dotPadding=self.model.dotPadding;conf.timeInterval=self.model.timeInterval;conf.animationDuration=self.model.animationDuration;conf.animationType=self.model.animation_type;conf.animationEasing=self.model.animation_easing;conf.showArrows=self.model.showArrows.toLowerCase()==="true";conf.showPageControl=self.model.showPageControl.toLowerCase()==="true";conf.stackSlides=self.model.stackSlides.toLowerCase()==="true";conf.animationSet=animSets[self.model.animation_type];conf.length=0;conf.current=self.model.startingSlide;conf.previous=undefined;conf.direction=CONST.forward;conf.locked=false;conf.isMobile=false;conf.isAnimating=false;conf.isMouseover=false;buildAnimationSet(self);}
var Timer=function(p){this.parent=p;this.time=p.conf.timeInterval;this.timer=0;};Timer.prototype={constructor:Timer,start:function(){if(!this.timer){this.timer=setInterval(this.t.bind(this),this.time);}},stop:function(){clearInterval(this.timer);this.timer=null;},t:function(){this.parent.forward();},reset:function(){this.stop();this.start();},destroy:function(){this.stop();this.parent=null;}};var Dot=function(p,m,i){var bPos,css;this.model=m;this.parent=p;this.hide={"top":addLengthUnit(this.model.h+this.model.padding)};this.show={"top":"0"};this.isActive=false;this.index=i;bPos=Math.floor(this.model.padding/2);css={"position":"absolute","background-image":this.model.off,"background-size":addLengthUnit(this.model.w)+" "+addLengthUnit(this.model.h),"background-position":addLengthUnit(bPos)+" "+addLengthUnit(bPos),"background-repeat":"no-repeat","left":"0","width":"100%","height":"100%","top":addLengthUnit(this.model.h+this.model.padding)};this.el=$(makeTag("div",false,"dot"));this.on=$(makeTag("span"));this.off=$(makeTag("span"));this.el.get(0).setAttribute("data-index",i);this.el.css({"position":"absolute","cursor":"pointer","padding":addLengthUnit(this.model.padding),"width":addLengthUnit(this.model.w),"height":addLengthUnit(this.model.h),"top":"0","overflow":"hidden","left":addLengthUnit(i*(this.model.w+this.model.padding))});css["background-image"]=this.model.on;this.on.css(css);css["background-image"]=this.model.off;this.off.css(css);this.el.append(this.on);this.el.append(this.off);this.parent.el.append(this.el);this.el.on("click",this.tapped.bind(this));if(wormwood.ad.device.isMobile===false){this.el.on("mouseover",this.onHover.bind(this));this.el.on("mouseout",this.onBlur.bind(this));}
this.deactivate();};Dot.prototype={constructor:Dot,activate:function(){this.off.css(this.hide);this.on.css(this.show);this.isActive=true;},deactivate:function(){this.on.css(this.hide);this.off.css(this.show);this.isActive=false;},tapped:function(e){this.parent.dotTapped(e.currentTarget.getAttribute("data-index"));},onHover:function(){if(!this.isActive){this.activate();}},onBlur:function(){if(this.isActive&&(parseInt(this.index)!==parseInt(this.parent.parent.conf.current))){this.deactivate();}},destroy:function(){this.el.off("click",this.tapped);if(wormwood.ad.device.isMobile===false){this.el.off("mouseover",this.onHover);this.el.off("mouseout",this.onBlur);}
this.el.remove();this.on.remove();this.off.remove();this.el=null;this.on=null;this.off=null;this.parent=null;}};function buildPageControl(self){var dot,i,model=self.parent.model,length=self.parent.conf.length,dotModel={w:model.dot_on.width(),h:model.dot_on.height(),on:model.dot_on.css("background-image"),off:model.dot_off.css("background-image"),padding:model.dotPadding};self.el=model.pageControl;self.el.css({"z-index":"10","width":addLengthUnit((dotModel.w+model.dotPadding)*length),"height":addLengthUnit(dotModel.h+model.dotPadding),"background-color":"rgba(0,0,0,0)","overflow":"hidden"});for(i=0;i<length;i++){self.dots.push(new Dot(self,dotModel,i));}
model.dot_on.remove();model.dot_off.remove();model.dot_on=null;model.dot_off=null;}
var PageControl=function(p){this.parent=p;this.dots=[];buildPageControl(this);};PageControl.prototype={constructor:PageControl,update:function(){this.dots[this.parent.conf.current].activate();if(typeof this.parent.conf.previous!=="undefined"){this.dots[this.parent.conf.previous].deactivate();}},dotTapped:function(i){this.parent.goTo(i);fireCallback(this.parent,{type:"onDotTap",index:i,el:this.dots[i]});},destroy:function(){var i;for(i=0;i<this.parent.conf;i++){this.dots[i].destroy();this.dots[i]=null;}
this.parent=null;}};var ArrowManager=function(p){var css={"z-index":"1"};this.parent=p;this.arrowLeft=this.parent.model.leftArrow;this.arrowRight=this.parent.model.rightArrow;this.arrowLeft.on("click",this.onLeft.bind(this));this.arrowRight.on("click",this.onRight.bind(this));this.arrowLeft.css(css);this.arrowRight.css(css);this.parent.model.leftArrow=null;this.parent.model.rightArrow=null;};ArrowManager.prototype={constructor:ArrowManager,onLeft:function(){this.parent.backwards();fireCallback(this.parent,{type:"onArrowTap",el:this.arrowLeft,});},onRight:function(){this.parent.forward();fireCallback(this.parent,{type:"onArrowTap",el:this.arrowRight,});},destroy:function(){this.arrowLeft.off("click",this.onLeft);this.arrowRight.off("click",this.onRight);this.arrowRight.remove();this.arrowLeft.remove();this.arrowRight=null;this.arrowLeft=null;this.parent=null;}};function buildSlides(self){var slides=[],slide,hide=(typeof self.parent.conf.animationSet.hideB!=="undefined")?self.parent.conf.animationSet.hideB.properties:self.parent.conf.animationSet.hideA.properties;self.parent.model.slideElements.each(function(i,e){slide=makeTag("div",false,"slide");$(slide).css({"position":"absolute","overflow":"hidden","left":"100%","top":"0","width":"100%","height":"100%","opacity":"0"}).on("click",self.imageTapped.bind(self));$v(slide).animate($.extend({"left":"0"},hide),{duration:0}).begin();$(e).css({"top":0,"left":0});$(slide).append(e);slides.push(slide);slide=null;});self.slideElements=$(slides);self.container.append(self.slideElements);self.parent.model.slideElements=null;slides=null;}
var SlideManager=function(p){var perspective=p.conf.animationSet.perspective||"none";this.parent=p;this.slideElements=undefined;this.slides=this.parent.model.slides;this.container=this.parent.model.container;this.container.css({"position":"absolute","left":"0","top":"0","width":addLengthUnit(this.parent.el.width()),"height":addLengthUnit(this.parent.el.height()),"overflow":"hidden","z-index":"0","-webkit-perspective":perspective,"-moz-perspective":perspective,"-ms-perspective":perspective,"-o-perspective":perspective,"perspective":perspective,"-webkit-transform-style":"preserve-3d","-moz-transform-style":"preserve-3d","-ms-transform-style":"preserve-3d","-o-transform-style":"preserve-3d","transform-style":"preserve-3d"});this.parent.model.slideElements.remove();this.parent.conf.length=this.parent.model.slideElements.length;this.parent.el.append(this.container);buildSlides(this);};SlideManager.prototype={constructor:SlideManager,update:function(){if(this.parent.conf.direction===CONST.forward){this.forwardAnimation.show.call(this);this.forwardAnimation.hide.call(this);}
else{this.backwardsAnimation.show.call(this);this.backwardsAnimation.hide.call(this);}},imageTapped:function(e){var symbol=this.slides[this.parent.conf.current];fireCallback(this.parent,{type:"onSlideTap",index:this.parent.conf.current,el:this.slideElements.eq(this.parent.conf.current)});if(symbol.onSlideTap){symbol.onSlideTap(e);}
symbol=null;},get:function(i){return this.slideElements.eq(i);},getAnim:function(baseName,anim){return(typeof anim[baseName+"B"]!=="undefined")?anim[baseName+"B"]:anim[baseName+"A"];},backwardsAnimation:{show:function(){var anim=this.parent.conf.animationSet,show=this.getAnim("show",anim),hide=this.getAnim("hide",anim),delay=show.delay||0,easing=show.easing||this.parent.conf.animationEasing,duration=this.parent.conf.animationDuration;this.show(show,hide,duration,delay,easing);},hide:function(){var anim,easing,duration,delay;if(typeof this.parent.conf.previous!=="undefined"){anim=this.parent.conf.animationSet;delay=anim.hideA.delay||0;easing=anim.hideA.easing||this.parent.conf.animationEasing;duration=this.parent.conf.animationDuration;this.hide(anim.hideA,duration,delay,easing);}}},forwardAnimation:{show:function(){var anim=this.parent.conf.animationSet,easing=anim.showA.easing||this.parent.conf.animationEasing,delay=anim.showA.delay||0,duration=this.parent.conf.animationDuration;this.show(anim.showA,anim.hideA,duration,delay,easing);},hide:function(){var anim,hide,easing,duration,delay;if(typeof this.parent.conf.previous!=="undefined"){anim=this.parent.conf.animationSet;hide=this.getAnim("hide",anim);delay=hide.delay||0;easing=hide.easing||this.parent.conf.animationEasing;duration=this.parent.conf.animationDuration;this.hide(hide,duration,delay,easing);}}},show:function(show,hide,duration,delay,easing){var self=this,el=this.slideElements.get(this.parent.conf.current),controller=this.slides.get(this.parent.conf.current),conf=this.parent.conf,opacity=(this.parent.conf.animationType==="fade")?{}:{"opacity":"1"},additionalShowStyles=show.initialStyles||{},additionalHideStyles=hide.initialStyles||{};if(controller.slideWillShow){controller.slideWillShow();}
if(this.parent.conf.firstRun===true){this.parent.conf.firstRun=false;duration=0;delay=0;}
