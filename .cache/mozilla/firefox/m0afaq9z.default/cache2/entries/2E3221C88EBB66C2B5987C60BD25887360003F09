this.slideElements.eq(this.parent.conf.current).css($.extend(additionalShowStyles,{"z-index":"1"}));$v(el).animate(hide.properties,{duration:0,}).begin();defer(function(){$v(el).animate(show.properties,{duration:duration,easing:easing,delay:delay}).begin({onComplete:function(){self.parent.conf.isAnimating=false;if(controller.slideDidShow){controller.slideDidShow();}
if(self.parent.conf.isMouseover===false){self.parent.startTimer();}
self=null;controller=null;}});el=null;},CONST.delay);},hide:function(hide,duration,delay,easing){var el=this.slideElements.get(this.parent.conf.previous),controller=this.slides[this.parent.conf.previous];if(controller.slideWillHide){controller.slideWillHide();}
if(this.parent.conf.stackSlides===true){delay=(duration+50);duration=0;}
defer(function(){$v(el).animate(hide.properties,{duration:duration,easing:easing,delay:delay}).begin({onComplete:function(){$(el).css({"opacity":"0","z-index":0});if(controller.slideDidHide){controller.slideDidHide();}
controller=null;el=null;}});},CONST.delay);},destroy:function(){this.container.remove();this.slideElements.remove();this.slides.remove();this.slideElements=null;this.container=null;this.parent=null;this.slides=null;}};var Gallery=function(view,model){this.model=model;this.view=view;};Gallery.prototype={version:"1.0.0",constructor:Gallery,init:function(callbacks){buildGallery(this,callbacks);},onReady:function(){if(this.callbacks.onReady){this.callbacks.onReady();}},onShowSlide:function(){var conf;if(this.callbacks.onShowSlide){conf=this.conf;this.callbacks.onShowSlide(conf.current,this.slides.get(conf.current),conf.direction);}},forward:function(){this.goTo(nextIndex(this),CONST.forward);},backwards:function(){this.goTo(prevIndex(this),CONST.backwards);},goTo:function(i,d){if(this.conf.isAnimating===false){var index=parseInt(i),dir=getDirection(this,i,d);if(dir){this.conf.isAnimating=true;this.conf.direction=dir;setIndex(this,i);beginAnimations(this);this.stopTimer();}}},startTimer:function(){if(this.timer){this.timer.start();}},stopTimer:function(){if(this.timer){this.timer.stop();}},resetTimer:function(){if(this.timer){this.timer.reset();}},getAnimationSet:function(name){return animSets[name];},addAnimationSet:function(name,anim){if(name&&anim){animSets[name]=anim;}},destroy:function(){if(this.timer){this.timer.destroy();}
